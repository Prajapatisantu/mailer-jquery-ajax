<%= form_with(url: product_path(product_id), method: :patch) do |f| %>
    <%= hidden_field_tag 'id', product_id %>
    <% product_id %>
    <div class="form-group">
      <%= f.rich_text_area :review, class:'form-control' %>
    </div>
    <br>
      <%= f.submit :submit,class:'btn btn-success' %>
<% end %>