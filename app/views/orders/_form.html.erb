<%= form_with(model:@order) do |f| %>
<div>
   <%= f.label :payment_type, class:'form-label' %> <%= f.text_field :payment_type, class:'form-control' %>
</div>
<br />
<div>
   <%= f.label :product, class:'form-check-label' %><br>
   <%= collection_check_boxes(:order, :product_ids, Product.all, :id, :name,class:"form-check-input")  %>
 </div>
<br />
<div>
   <%= f.submit :submit,class:'btn btn-success' %>
</div>
<% end %>




 